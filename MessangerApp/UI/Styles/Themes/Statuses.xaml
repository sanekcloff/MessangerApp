<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--#region Statuses-->
    <LinearGradientBrush x:Key="Offline" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Color="#afafaf" Offset="0"/>
        <GradientStop Color="#7d7d7d" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="Online" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Color="#3E9A39" Offset="0"/>
        <GradientStop Color="#286925" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="IsMoveAway" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Color="#e4b83e" Offset="0"/>
        <GradientStop Color="#dea032" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="IsBussy" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Color="#ce3030" Offset="0"/>
        <GradientStop Color="#af2727" Offset="1"/>
    </LinearGradientBrush>
    <!--#endregion-->

</ResourceDictionary>