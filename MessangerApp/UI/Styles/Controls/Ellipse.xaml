<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ImageBrush x:Key="Patch" ImageSource="pack://application:,,,/UI;component/Assets/Icons/Controls/Patch/userPatch.png" Stretch="UniformToFill"/>
    
    <Style x:Key="UploadedImage" TargetType="Ellipse">
        <Setter Property="Fill" Value="{StaticResource Patch}"/>
        <Setter Property="Margin" Value="5"/>
        
    </Style>


</ResourceDictionary>