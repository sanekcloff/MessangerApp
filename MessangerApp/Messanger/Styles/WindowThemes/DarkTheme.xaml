<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--#region Window colors-->
    <SolidColorBrush x:Key="WindowPrimaryColor" Color="#1f1f1f"/>
    <SolidColorBrush x:Key="WindowSecondaryColor" Color="#1e1e1e"/>
    <SolidColorBrush x:Key="WindowAccentColor" Color="#1e1e1e"/>
    <!--#endregion-->
    <!--#region Text colors-->
    <SolidColorBrush x:Key="TextPrimaryColor" Color="#ffffff"/>
    <SolidColorBrush x:Key="TextSecondaryColor" Color="#5a5a5a"/>
    <SolidColorBrush x:Key="TextAccentColor" Color="#ffffff"/>
    <!--#endregion-->
    <!--#region Button-->
    <SolidColorBrush x:Key="ButtonPrimaryColor" Color="#3d3d3d"/>
    <SolidColorBrush x:Key="ButtonSecodaryColor" Color="#2e2e2e"/>
    <SolidColorBrush x:Key="ButtonAccentColor" Color="#7160e8"/>
</ResourceDictionary>