<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--#region Window colors-->
    <SolidColorBrush x:Key="WindowPrimaryColor" Color="#1e1e1e"/>
    <SolidColorBrush x:Key="WindowSecondaryColor" Color="#383838"/>
    <SolidColorBrush x:Key="WindowAccentColor" Color="#7160e8"/>
    <!--#endregion-->
    <!--#region Text colors-->
    <SolidColorBrush x:Key="TextPrimaryColor" Color="#c8dcdc"/>
    <SolidColorBrush x:Key="TextSecondaryColor" Color="#c8dcdc"/>
    <!--#endregion-->
</ResourceDictionary>